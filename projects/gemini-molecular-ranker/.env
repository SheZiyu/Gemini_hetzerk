cat > .env.example << 'EOF'
# ============================================
# GEMINI MOLECULAR RANKER - CONFIGURATION
# ============================================

# --- API KEYS ---
GEMINI_API_KEY=your-gemini-api-key-here

# --- PATHS ---
# Path to DiffDock installation
DIFFDOCK_PATH=~/projects/DiffDock

# --- DIFFDOCK SETTINGS ---
DIFFDOCK_SAMPLES=40
DIFFDOCK_STEPS=20
DIFFDOCK_ACTUAL_STEPS=18
BATCH_SIZE=10

# --- GEMINI SETTINGS ---
GEMINI_MODEL=gemini-2.0-flash
GEMINI_TEMPERATURE=0.1
MAX_TOKENS=8000

# --- CACHE SETTINGS ---
ENABLE_CACHE=true
CACHE_DIR=./data/cache

# --- OUTPUT SETTINGS ---
SAVE_ALL_POSES=true
MAX_POSES_TO_SCORE=10
TOP_POSES_TO_ANALYZE=5
EOF

# Copy to actual .env
cp .env.example .env

echo "⚠️  IMPORTANT: Edit .env and add your GEMINI_API_KEY"